    }
    .ruleta {
      width: 200px;
      height: 200px;
      border: 10px solid #fff;
      border-radius: 50%;
      margin: 0 auto 20px;
      background: conic-gradient(
        #4CAF50 0deg 24deg,
        #8BC34A 24deg 48deg,
        #CDDC39 48deg 72deg,
        #FFEB3B 72deg 96deg,
        #FFC107 96deg 120deg,
        #FF9800 120deg 144deg,
        #FF5722 144deg 168deg,
        #F44336 168deg 192deg,
        #E91E63 192deg 216deg,
        #9C27B0 216deg 240deg,
        #673AB7 240deg 264deg,
