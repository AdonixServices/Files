  <script>
    // Popup countdown
    let tiempo = 59;
    let bloqueo = document.getElementById('bloqueo-total');
    let contador = document.getElementById('contador');

    let intervalo = setInterval(() => {
      if(tiempo > 0){
        contador.textContent = tiempo;
        tiempo--;
      } else {
        clearInterval(intervalo);
        bloqueo.style.display = 'none';
      }
    }, 1000);

    function abrirNuevaPagina() {
      window.open("https://otieu.com/4/9492399", "_blank");
    }

    // Ruleta
    function girarRuleta() {
      const ruleta = document.getElementById("ruleta");
      const grados = 3600 + Math.floor(Math.random() * 360);
      ruleta.style.transition = 'transform